extends base
block head
	//- inject:css
	link(rel="stylesheet", href="/garbarino-front/build/garbarino-front-lib.css")
	link(rel="stylesheet", href="/garbarino-front/build/garbarino-front.css")
	//- endinject
block footer
	//- inject:js
	script(src="/garbarino-front/build/garbarino-front-lib.js")
	script(src="/garbarino-front/build/garbarino-front.js")
	//- endinject
	script(type='text/javascript' src='//code.jquery.com/jquery-3.2.1.min.js')
	script(type='text/javascript' src='//cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js')
	script(type='text/javascript' src="//cdnjs.cloudflare.com/ajax/libs/angular-i18n/1.6.6/angular-locale_#{domain.locale.toLowerCase()}.min.js")
	script.
		(function () {
			angular.bootstrap(document, ['garbarinofront'], {strictDi: true});
			$(document).ready(function(){
				$('.modal').modal({
					dismissible: false
				});
			});
		})();

